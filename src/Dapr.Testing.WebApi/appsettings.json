{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://*:8081",
        "Protocols": "Http1"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Grpc": "Warning"
      }
    },
    "Properties": {
      "Application": "dapr.testing.web-api",
      "Environment": "dev"
    }
  },
  "AllowedHosts": "*"
}
